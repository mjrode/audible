{
  "name": "audible-backend",
  "version": "0.0.0",
  "description": "The backend API for a project build with Nest.js and React",
  "author": "Michael Rode",
  "license": "MIT",
  "scripts": {
    "build": "nest build",
    "prebuild": "rimraf dist",
    "start:dev:db": "sh ./src/scripts/start-db.sh",
    "lint": "eslint . --cache --ext=.ts,.tsx,.js,.jsx",
    "lint-fix": "npm run lint -- --fix",
    "db": "cd .. && docker-compose up -d",
    "restart:db": "cd .. && docker-compose rm -s -f && docker-compose up -d",
    "db:log": "npm run build && typeorm schema:log",
    "db:show": "npm run build && typeorm migration:show",
    "db:generate": "npm run build && typeorm migration:generate -n",
    "db:migrate": "npm run build && typeorm migration:run",
    "release": "semantic-release --debug",
    "start": "nest start --watch",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/src/main",
    "test": "jest --runInBand",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "tsc": "tsc"
  },
  "dependencies": {
    "@nest-middlewares/cors": "^6.0.1",
    "@nest-middlewares/helmet": "^6.0.0",
    "@nestjs/common": "^6.11.7",
    "@nestjs/core": "^6.11.7",
    "@nestjs/jwt": "^6.1.1",
    "@nestjs/passport": "^6.2.0",
    "@nestjs/platform-express": "^6.11.11",
    "@nestjs/typeorm": "^6.3.4",
    "@typescript-eslint/eslint-plugin": "^2.34.0",
    "ajv": "^6.12.2",
    "auth0": "^2.27.0",
    "axios": "^0.19.2",
    "cheerio": "^1.0.0-rc.3",
    "class-transformer": "^0.2.3",
    "class-validator": "^0.11.1",
    "cors": "^2.8.5",
    "custom-env": "^2.0.1",
    "dotenv": "^8.2.0",
    "dotenv-flow": "^3.2.0",
    "es6-promisify": "^6.1.1",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-react": "^7.20.3",
    "eslint-plugin-react-hooks": "^1.7.0",
    "express-jwt": "^5.3.3",
    "express-list-routes": "^0.1.4",
    "fast-memoize": "^2.5.2",
    "googleapis": "^39.2.0",
    "http-proxy-middleware": "^1.0.4",
    "jwks-rsa": "^1.8.1",
    "open": "^7.0.4",
    "passport": "^0.4.1",
    "passport-google-oauth20": "^2.0.0",
    "passport-http-bearer": "^1.0.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "pg": "^7.18.2",
    "reflect-metadata": "^0.1.12",
    "rimraf": "^2.7.1",
    "rxjs": "^6.6.0",
    "shelljs": "^0.8.4",
    "transmission": "^0.4.10",
    "transmission-api": "0.0.1",
    "transmission-promise": "^1.1.4",
    "typeorm": "^0.2.25",
    "typeorm-fixtures-cli": "^1.7.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^6.11.3",
    "@nestjs/schematics": "^6.7.0",
    "@nestjs/testing": "^6.11.11",
    "@types/auth0": "^2.20.5",
    "@types/cheerio": "^0.22.18",
    "@types/es6-promisify": "^6.0.0",
    "@types/express": "^4.17.6",
    "@types/express-jwt": "0.0.42",
    "@types/jasmine": "^3.5.11",
    "@types/jest": "^24.0.18",
    "@types/node": "^12.12.47",
    "@types/passport-jwt": "^3.0.3",
    "@types/prettier": "^1.19.1",
    "@types/rimraf": "^2.0.4",
    "@types/supertest": "^2.0.10",
    "@typescript-eslint/parser": "^2.34.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-typescript": "^7.2.1",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jest": "^23.17.1",
    "eslint-plugin-prettier": "^3.1.4",
    "jest": "^26.1.0",
    "nock": "^13.0.2",
    "nock-record": "^0.3.8",
    "nodemon": "^1.18.3",
    "pre-commit": "^1.2.2",
    "prettier": "^1.14.2",
    "supertest": "^4.0.2",
    "ts-jest": "^26.1.1",
    "ts-loader": "^6.2.2",
    "ts-mockito": "^2.6.1",
    "ts-node": "^8.10.2",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.9.6",
    "webpack-node-externals": "^1.7.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "setupFiles": [
      "dotenv-flow/config"
    ],
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
